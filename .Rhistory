# Define the function to generate unique combinations of methods
generate_unique_combinations <- function(methods) {
all_combinations <- list()
for (k in 1:length(methods)) {
combs <- combn(methods, k, FUN = function(x) paste(sort(x), collapse = ","), simplify = FALSE)
all_combinations <- c(all_combinations, combs)
}
unique_combinations <- unique(unlist(all_combinations))
strsplit(unique_combinations, ",")
}
# Generate unique combinations of methods where order doesn't matter
unique_combinations <- generate_unique_combinations(df$Method)
unique_combinations
